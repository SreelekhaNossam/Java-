// write your code here
var img = new SimpleImage("duke_blue_devil.png");
print(img);
for (var pixel of img.values()){
    r=pixel.getRed();
    g=pixel.getGreen();
    b=pixel.getBlue();
    if (r!=255 && g!=255 && b!=255){
        pixel.setRed(255);
        pixel.setGreen(255);
        pixel.setBlue(0);
    }
}
print(img);
